{
    "name": "tg-be-2",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "description": "backend for tugasguru",
    "scripts": {
        "start": "node dist/server",
        "clean": "rm -rf dist",
        "build": "npm -s run clean && npm -s run generate && tsc",
        "generate": "pal g && npm -s run generate:prisma && npm -s run generate:nexus",
        "generate:prisma": "prisma generate",
        "generate:nexus": "ts-node --transpile-only src/nexusSchema",
        "db-dev": "prisma migrate dev",
        "dev": "ts-node-dev --no-notify --respawn --transpile-only server",
        "seed": "yarn prisma migrate reset --force && yarn ts-node-dev seed.ts"
    },
    "dependencies": {
        "@graphql-tools/schema": "^8.3.11",
        "@paljs/nexus": "4.1.0",
        "@prisma/client": "3.14.0",
        "@types/bcrypt": "^5.0.0",
        "@types/graphql-upload": "^8.0.11",
        "@types/jsonwebtoken": "^8.5.8",
        "apollo-server": "^3.7.0",
        "apollo-server-core": "^3.7.0",
        "apollo-server-express": "^3.7.0",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "common-tags": "^1.8.2",
        "dotenv": "^16.0.1",
        "fast-glob": "^3.2.11",
        "graphql": "^16.5.0",
        "graphql-middleware": "^6.1.26",
        "graphql-rate-limit": "^3.3.0",
        "graphql-scalars": "^1.17.0",
        "graphql-subscriptions": "^2.0.0",
        "graphql-upload": "^13.0.0",
        "graphql-ws": "^5.8.2",
        "ioredis": "^5.0.4",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "nexus": "1.3.0",
        "uWebSockets.js": "uNetworking/uWebSockets.js#v20.10.0",
        "uuidjs": "^4.2.11",
        "ws": "^8.6.0"
    },
    "devDependencies": {
        "@paljs/cli": "4.1.0",
        "@types/node": "15.14.9",
        "prettier": "2.6.2",
        "prisma": "3.14.0",
        "ts-node": "10.7.0",
        "ts-node-dev": "1.1.8",
        "typescript": "4.6.4"
    },
    "prettier": {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "all"
    },
    "repository": "tg-be",
    "author": "damar"
}